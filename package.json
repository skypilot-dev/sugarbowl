{
  "name": "@skypilot/sugarbowl",
  "version": "0.10.4",
  "description": "A collection of convenience functions for JavaScript & TypeScript",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rm -rf lib && babel ./src -d ./lib --extensions .ts --ignore '**/*.test.ts'",
    "check-types": "tsc --noEmit",
    "generate-typings": "tsc --project tsconfig.generate-typings.json",
    "prepublishOnly": "yarn check-types && yarn test && yarn build && yarn generate-typings",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/skypilotcc/sugarbowl.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "utility",
    "functions",
    "arrays",
    "strings",
    "objects"
  ],
  "author": "William Thorsen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/skypilotcc/sugarbowl/issues"
  },
  "homepage": "https://github.com/skypilotcc/sugarbowl#readme",
  "dependencies": {},
  "devDependencies": {},
  "files": [
    "/lib"
  ]
}
