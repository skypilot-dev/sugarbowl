{
  "name": "@skypilot/sugarbowl",
  "version": "0.7.1",
  "description": "A collection of convenience functions for JavaScript & TypeScript",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rm -rf lib && babel ./src -d ./lib --extensions .ts --ignore '**/*.test.ts'",
    "check-types": "tsc --noEmit",
    "generate-typings": "tsc --project tsconfig.generate-typings.json",
    "prepublishOnly": "npm test && npm run build && npm run generate-typings",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/skypilotcc/sugarbowl.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "utility",
    "functions",
    "arrays",
    "strings",
    "objects"
  ],
  "author": "William Thorsen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/skypilotcc/sugarbowl/issues"
  },
  "homepage": "https://github.com/skypilotcc/sugarbowl#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.5.5",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-plugin-jest": "^22.13.6",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.3"
  },
  "files": [
    "/lib"
  ]
}
