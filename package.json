{
  "name": "@skypilot/sugarbowl",
  "version": "0.10.9",
  "description": "A collection of convenience functions for JavaScript & TypeScript",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "all-ci-checks": "yarn run all-cq-checks && yarn run build",
    "all-cq-checks": "yarn run check-types && yarn run lint --quiet && yarn test",
    "build": "rm -rf lib && yarn run compile-ts",
    "check-types": "tsc",
    "compile-ts": "babel ./src --out-dir ./lib --extensions .ts --ignore '**/*.test.ts'",
    "generate-typings": "tsc --project tsconfig.generate-typings.json",
    "lint": "eslint --cache '**/*.{js,ts}'",
    "prepublishOnly": "yarn run check-types && yarn run lint --quiet && yarn test && yarn run build && yarn run generate-typings",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/skypilotcc/sugarbowl.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "utility",
    "functions",
    "arrays",
    "strings",
    "objects"
  ],
  "author": {
    "name": "William Thorsen",
    "email": "wthorsen@skypilot.dev"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/skypilotcc/sugarbowl/issues"
  },
  "homepage": "https://github.com/skypilotcc/sugarbowl#readme",
  "dependencies": {},
  "devDependencies": {
    "@skypilot/toolchain": "^1.0.0"
  },
  "files": [
    "/lib"
  ]
}
